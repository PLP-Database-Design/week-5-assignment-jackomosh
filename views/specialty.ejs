<table>
    <thead>
        <tr>
            <th>Provider ID</th>
            <th>Provider Specialty</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row => {%>
            <tr>
                <td><%= row.provider_id %></td>
                <td><%= row.provider_specialty %></td>
            </tr>
        <% }) %>
    </tbody>
</table>